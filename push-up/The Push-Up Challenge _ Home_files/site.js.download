$(function(){
    $('#template-profile .profileSidebar .profileSponsorForm').addClass('float-center mb20').css('width','350px');
    $("[role=tab]").click(function(){
        if($(this).attr("data-bs-target")){
            var labelledby = $(this).attr("data-bs-target").replace("#","") + "-tab";
            $(this).closest(".nav").find(".active[role='tab']").removeClass("active");
            $(this).addClass("active");
            $("[aria-labelledby='" + labelledby + "']").closest(".tab-content").find(".active[role='tabpanel']").removeClass("active");
            $("[aria-labelledby='" + labelledby + "']").addClass("active");
        }
    });
    
    <!--Remove this later-->
    if(new URLSearchParams(window.location.search).get('garfield') != null){$(".dev-garfield").css("display","block");}

});
